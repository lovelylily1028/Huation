.ellipsis{display:inline-block;text-overflow:ellipsis;overflow:hidden;white-space:nowrap;}
.w120{width:120px;}

#bbs_wrap .list_photo ul li.deleted .ph_cont a, #bbs_wrap .list_photo ul li.deleted .ph_cont span.ph_date{color:#bfbfbf;}
#bbs_wrap .deleted .tit a{color:#bfbfbf;}

/* prev_next */
.preview dl{overflow:hidden;width:100%;border-top:1px solid #d1d1d1;border-bottom:1px solid #d1d1d1;margin-top:-1px;}
.preview dt{float:left;width:12%;height:30px;font-weight:bold;background:#efefef;line-height:30px;vertical-align:middle;padding-left:5%;}
.preview dd{float:left;width:78%;}

/* photo view */
.thumbnail_photo{overflow:hidden;padding:20px 0 20px 10px;border-top:1px solid #ccc;border-bottom:1px solid #ccc;}
.thumbnail_photo li{float:left;display:block;padding:0 10px;margin-bottom:20px;width:120px;height:80px;}
.thumbnail_photo li a{display:block;background:#efefef;}
.thumbnail_photo li a img{width:120px;height:80px;}

/* book_list */
.today_month{margin-bottom:30px;font-size:22px;color:#59739d;text-align:center;}
.today_month strong{margin:0 55px;}
.out_line{padding:10px 0;border-top:1px solid #d8d8d8;border-bottom:1px solid #d8d8d8;}
.div_line{border-top: 1px solid #d8d8d8;}
.book_list{overflow:hidden;width:100%;padding:25px 0;}
.book_list li{position:relative;float:left;width:342px;height:136px;padding:10px;margin:0 5px 20px;border:1px dashed #c6c6c6;background:#f9f9f9 url('./images/bg_book.gif') right bottom no-repeat;}
.book_list li .tit{display:block;width:90%;height:27px;padding:0 5%;margin-bottom:10px;border:1px solid #728baf;color:#4d678c;background:#fafafa;line-height:27px;;}
.book_list li .tit span{display:block;width:80%;overflow:hidden;text-overflow:ellipsis;white-space:nowrap}
.book_list li .more{position:absolute;top:17px;right:17px;color:#4d678c;font-weight:bold;padding-right:10px;font-size:10px;background:url('./images/bg_more.gif') right center no-repeat;}
.book_list li .book_box{overflow:hidden;width:100%;}
.book_list li .book_box .img{float:left;display:block;width:85px;height:95px;margin-right:15px;background:#fff;}
.book_list li .book_box .img img{width:85px;height:95px;}
.book_list li .book_box  .book_info .ctg{display:block;color:#454545;padding:5px 0;}
.book_list li .book_box  .book_info .ctg span{color:#ed7e07;}
.book_list li .book_box  .book_info .book_info_cont span{display:block;color:#2b2b2b;}
.book_list li .book_box  .book_info .book_info_cont p{height:30px;overflow:hidden;min-height:80px;}

/* book_view */
.book_view{border-top:2px solid #4d678c;}
.book_view .bview_info{overflow:hidden;width:100%;}
.book_view .bview_info .imgview{float:left;width:165px;padding:10px;text-align:center}
.book_view .bview_info .imgview img{display:block;width:165px;background:#efefef;}
.book_view .bview_info .infoview{float:left;width:565px;}
.book_view .bview_info .infoview table{border-collapse:collapse;width:100%;}
.book_view .bview_info .infoview table tbody th{padding:10px;background:#f5f5f5;color:#636363;border-bottom:1px solid #d9d9d9}
.book_view .bview_info .infoview table tbody td{padding:10px;border-bottom:1px solid #d9d9d9}
.book_view .bview_info .infoview table tbody td.tit{font-weight:bold;font-size:13px;color:#636363}
.book_view_cont{padding:30px 20px;margin-top:-1px;border-top:1px solid #d9d9d9;border-bottom:1px solid #d9d9d9}

.notice_cont{margin:10px 0; height:400px;padding:0 20px 0 20px;overflow-y:scroll;border:1px solid #d2d2d2;background:#fff;}

