@CHARSET "UTF-8";
/***********************************************************
*  bbs_warp : web 
************************************************************/
#bbs_wrap {width:100%;}
#bbs_wrap .total {margin-bottom: 10px;text-align:left;width:auto;float:none;color:#7f7f7f;}
#bbs_wrap .bbs_list {width:100%; }
#bbs_wrap #bbs_search input { vertical-align: middle; }
#bbs_wrap #bbs_search {background:#faf9f9;border:1px solid #e9e9e9;line-height: 37px; margin-bottom: 30px; padding:3px 0 7px;text-align: center; }
#bbs_wrap #bbs_search .inp_s { border: 1px solid #dddddd; height: 21px; line-height: 21px; margin: 0 5px; width:32%; }
#bbs_wrap #bbs_search select { border: 1px solid #7F9DB9; vertical-align: middle; }

/* board_paging */
#bbs_wrap #paging { width: 100%; color: #7F7F7F;  font-style: normal; margin: 20px 0; font-weight: bold; text-align: center; }
#bbs_wrap #paging a {margin:0 5px;}
#bbs_wrap #paging img {vertical-align:middle;}
#bbs_wrap #paging a {display:inline-block;  line-height:18px; vertical-align:middle;}
#bbs_wrap #paging a span {position:absolute; z-index:-1;visibility:hidden;}
#bbs_wrap #paging .first , #bbs_wrap #paging .prev , #bbs_wrap #paging .next , #bbs_wrap #paging .last{background-image:url('./images/btn_all.gif');width:21px; height:20px;}
#bbs_wrap #paging .first {position:relative; background-position:0 -270px;width:24px;}
#bbs_wrap #paging .prev {position:relative;background-position:-30px -270px;}
#bbs_wrap #paging .next {position:relative; background-position:-61px -270px; }
#bbs_wrap #paging .last {position:relative; background-position:-90px -270px;width:24px;}
#bbs_wrap #paging .current {display:inline-block; margin: 0 5px; vertical-align:middle;color:#000}

#bbs_wrap .blue{color: #4d678c;}
#bbs_wrap .vMid{vertical-align:middle;}
#bbs_wrap .btn_all{overflow:hidden;width:100%;padding:20px 0;}
#bbs_wrap .btn_all .fL{float:left;width:70%;}
#bbs_wrap .btn_all .fR{float:right;width:29%;text-align:right;}
#bbs_wrap .btn_c{text-align:center;padding:20px 0;}
#bbs_wrap .btn_r{text-align:right;padding:20px 0;}


/* list FAQ */
#bbs_wrap .list_faq .tit_faq{overflow:hidden;height:32px;line-height:32px;vertical-align:middle;background:
#edf0f4;border-top:2px solid #4d678c;border-bottom:1px solid #d9d9d9;color:#515151;}
#bbs_wrap .list_faq .tit_faq strong{float:left;display:block;text-align:center;height:32px;line-height:32px;}
#bbs_wrap .list_faq .tit_faq .num{width:50px;}
#bbs_wrap .list_faq .tit_faq .tit{width:700px;}
#bbs_wrap .list_faq{}
#bbs_wrap .list_faq dt{overflow:hidden;width:100%;min-height:25px;padding:5px 0;border-bottom:1px solid #d9d9d9;}
#bbs_wrap .list_faq dt .num{float:left;display:block;width:50px;text-align:center;}
#bbs_wrap .list_faq dt .question{float:left;display:block;width:40px;height:24px;text-indent:-1000em;background:url('./images/icon_question.gif') center 0 no-repeat;}
#bbs_wrap .list_faq dt a{float:left;display:block;height:24px;padding:0 15px;}
#bbs_wrap .list_faq dd{overflow:hidden;width:100%;padding:20px 0;border-bottom:1px solid #d9d9d9;background:#fafafa;}
#bbs_wrap .list_faq dd span{float:left;display:block;width:40px;height:24px;margin-left:50px;text-indent:-1000em;background:url('./images/icon_answer.gif') center 0 no-repeat;}
#bbs_wrap .list_faq dd div{float:left;padding:0 15px;}

/* list 일반 */
#bbs_wrap .list_table {width: 100%;border-collapse: collapse;border-top:2px solid #4d678c;color:#515151; table-layout: fixed;}
#bbs_wrap .list_table thead th{background: #edf0f4;color:#515151;border-bottom:1px solid #d9d9d9;}
#bbs_wrap .list_table th {line-height:35px;}
#bbs_wrap .list_table td {border-bottom:1px solid #d9d9d9;}
#bbs_wrap .list_table .check {width:9.5%;}
#bbs_wrap .list_table .num {width:7.5%;}
#bbs_wrap .list_table .class{width: 10%;}
#bbs_wrap .list_table .tit {padding-left:1%; overflow:hidden; text-align:left; min-width:35px;} /*text-overflow:ellipsis;white-space:nowrap;}*/
#bbs_wrap .list_table th.tit {text-align:center;}
#bbs_wrap .list_table .tit a{color:#000;}/*text-overflow:ellipsis;white-space:nowrap;}*/
#bbs_wrap .list_table .state {width: 9%;}
#bbs_wrap .list_table .writer {width: 10%;}
#bbs_wrap .list_table .file {width:6.5%;}
#bbs_wrap .list_table .date {width: 13%;}
#bbs_wrap .list_table .hits {width: 7.5%;}
#bbs_wrap .list_table tbody td {text-align:center; line-height:35px;}
#bbs_wrap .list_table tbody td.tit {text-align:left; line-height:15px;}
/*#bbs_wrap .list_table tbody td.tit a{display:inline-block; white-space:nowrap; overflow:hidden; max-width:280px; text-overflow:ellipsis; vertical-align:middle;}*/
/*#bbs_wrap .list_table tbody td.tit em{vertical-align:middle;}*/
#bbs_wrap .list_table tbody td.tit .box {display: inline-block; max-width: 98%;}
#bbs_wrap .list_table tbody td.tit .box .secret {display: block; overflow: hidden; white-space: nowrap; color: #3d3d3d; text-decoration: none; text-overflow: ellipsis;}
#bbs_wrap .list_table tbody td.tit .box a{display: block; overflow: hidden; white-space: nowrap; color: #3d3d3d; text-decoration: none; text-overflow: ellipsis;}
#bbs_wrap .list_table tbody td.tit .box em{float: right; }
#bbs_wrap .list_table tbody td.tit .box img{float: right; margin-top: 2px; margin-left: 2px;}
#bbs_wrap .list_table tbody tr.deleted td.tit a{color:#dfdfdf;text-decoration: line-through;}


#bbs_wrap .list_table .notice .class {font-weight:bold;color:#5978AF;}

#bbs_wrap #paging {float:none;}



/* wirte */
#bbs_wrap .board_write{width:100%;border-top:2px solid #4d678c;}
#bbs_wrap .board_write dl{overflow:hidden;width:100%;background:#F6F6F6; }
#bbs_wrap .board_write dt{float:left;width:17%;padding:4px 0 5px 3%; min-height:23px;line-height:23px;vertical-align:middle;background:#F6F6F6;font-weight:bold;border-bottom: 1px solid #d1d1d1;}
#bbs_wrap .board_write dd{float:left;width:77%;padding:4px 0 5px 3%;min-height:23px;line-height:23px;vertical-align:middle;background:#fff;border-bottom: 1px solid #d1d1d1;}
#bbs_wrap .board_write dd input.tit{width:95%;line-height:20px;vertical-align:middle;border:1px solid #d1d1d1;}
#bbs_wrap .board_write dd select{border: 1px solid #d1d1d1;}
#bbs_wrap .board_write dt.file, .board_write dd.file{border-bottom:none;}
#bbs_wrap .board_write dd input.file_up{border: 1px solid #d1d1d1;width:95%;}

#bbs_wrap .board_write dd ul.file_list{padding:3px 0;}
#bbs_wrap .board_write dd ul.file_list li{padding:2px 0;line-height:14px;}
#bbs_wrap .board_write dd ul.file_list li img{vertical-align:middle;}

#bbs_wrap .board_write dd.write_cont{width:100%;padding:0;border-top: 1px solid #d1d1d1;border-bottom: 1px solid #d1d1d1;}
#bbs_wrap .board_write dd.write_cont textarea{width:100%;min-height:400px;border:1px solid #d1d1d1;font-size:12px;}


 /* view */

#bbs_wrap .board_view dl{overflow:hidden;width:100%;}
#bbs_wrap .board_view dt{float:left;min-height:35px;line-height:35px;font-weight:bold;color:#6e6e6e;background:#f5f5f5;border-bottom:1px solid #d9d9d9;}
#bbs_wrap .board_view dd{float:left;min-height:35px;line-height:35px;vertical-align:middle;border-bottom:1px solid #d9d9d9;}

#bbs_wrap .tit_view{overflow:hidden;width:100%;border-top:2px solid #4d678c;}
#bbs_wrap .tit_view2{overflow:hidden;width:100%;border-top:2px solid #00AA8F;}
#bbs_wrap .tit_view dt{float:left;width:16%;padding-left:2%;}
#bbs_wrap .tit_view2 dt{float:left;width:16%;padding-left:2%;}
#bbs_wrap .tit_view dd{float:left;width:80%;padding-left:2%;font-weight:bold;}
#bbs_wrap .tit_view2 dd{float:left;width:80%;padding-left:2%;font-weight:bold;}

#bbs_wrap .info_view dt{width:16%;padding-left:2%;border-bottom:1px solid #d9d9d9;}
#bbs_wrap .info_view dd{width:80%;padding-left:2%;border-bottom:1px solid #d9d9d9;}

#bbs_wrap .info_view2 dt{width:12%;text-align:center;}
#bbs_wrap .info_view2 dd{width:10%;padding-left:2%;}
#bbs_wrap .info_view2 dt.writer{width:16%;padding-left:2%;text-align:left;}
#bbs_wrap .info_view2 dd.writer{width:32%;padding-left:2%;}

#bbs_wrap .file_view{background:#f5f5f5;}
#bbs_wrap .file_view dt{width:16%;padding-left:2%;border-bottom:none;}
#bbs_wrap .file_view dd{width:80%;padding-left:2%;background:#fff;}
#bbs_wrap .file_view dd ul{padding:3px 0;}
#bbs_wrap .file_view dd ul li{padding:2px;line-height:14px;}
#bbs_wrap .file_view dd ul li img{vertical-align:middle;}

#bbs_wrap .tit_list{background:#F9F9F9;border-top:1px solid #d9d9d9;}
#bbs_wrap .tit_list dt{width:15%;padding-left:5%;border-bottom:1px solid #d9d9d9;}
#bbs_wrap .tit_list dd{width:80%;border-bottom:1px solid #d9d9d9;}

#bbs_wrap .board_view .view_cont{padding:20px;margin-top:-1px;border-top:1px solid #d9d9d9;border-bottom:1px solid #d9d9d9;min-height:100px;word-break:break-all;}
#bbs_wrap .board_view .view_cont em {font-style:italic;}

/* 댓글 */
#bbs_wrap .comment{padding:10px 0;}
#bbs_wrap .comment_inp{overflow:hidden;width:100%;}
#bbs_wrap .comment_inp strong{float:left;display:block;width:auto;padding:0 5px;}
#bbs_wrap .comment_inp textarea{float:left;width:75%;margin-right:2%;height: 40px;padding: 3px;background-color: #F6F6F6;border: 1px solid #CACACA;color: #767676;font-size: 12px;}
#bbs_wrap .comment p{padding:5px 0 0 10%;color: #C57878;}
#bbs_wrap .commentPopup {border-bottom: 1px dashed #D7D7D7;}


#bbs_wrap .reply{padding: 10px 0 7px;}
#bbs_wrap .reply strong{float:left;color:#000;font-weight:bold;}
#bbs_wrap .reply span{float:right; color: #A4A4A4;display: inline-block; font: 11px/13px Tahoma;padding: 0;}
#bbs_wrap .reply span img{vertical-align:middle;margin:0 2px;}
#bbs_wrap .reply p.reply_cont{clear:both;padding: 5px 0;color: #A4A4A4;font: 12px/18px Dotum,"돋움";word-break: break-all;}

#bbs_wrap .type1{border-bottom: 1px dashed #D7D7D7;}
