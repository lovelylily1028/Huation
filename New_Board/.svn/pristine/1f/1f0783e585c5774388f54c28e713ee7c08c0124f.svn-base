#login_page .btn_s, #login_page .btn_s span, #login_page .btn_s button{position:relative;background:url('/template/member/images/login/bg_btn.gif');}
#login_page .btn_s{display:inline-block;height:23px;line-height:23px;margin-right:3px;cursor:pointer;background-position:0 0;vertical-align:top;}
#login_page .btn_s span, #login_page .btn_s button{left:3px;display:inline-block;height:23px;line-height:23px;margin:0;padding:0 15px 0 12px;font-size:12px;text-align:center;color:#696969;font-weight:normal;background-position:right 0;vertical-align:middle;}
#login_page .btn_s span span{padding:0;}


/* btn_content */
#login_page .btn0, #login_page.btn1, #login_page .btn2, #login_page .lbtn, #login_page .lbtn1, #login_page .lbtn2, #login_page #login_page .lbbtn1,
#login_page .btn0 a, #login_page .btn1 a, #login_page .btn2 a, #login_page .lbtn a, #login_page .lbtn1 a, #login_page .lbtn2 a, #login_page .lbbtn1 a,
#login_page .btn0 input, #login_page .btn1 input, #login_page .btn2 input, #login_page .lbtn input, #login_page .lbtn1 input, #login_page .lbtn2 input, #login_page .lbbtn1 input,
#login_page .btn0 button, #login_page .btn1 button, #login_page .btn2 button, #login_page .lbtn button, #login_page .lbtn1 button, #login_page .lbtn2 button, #login_page .lbbtn1 button{position:relative;white-space: nowrap;background-image:url('/template/member/images/login/btn_content.gif');vertical-align:middle;}
