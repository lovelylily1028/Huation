#two_tabs{text-align:center;width:100%;height:40px;margin-bottom:20px;}
#two_tabs div{font-size:15px;float:left;width:49%;line-height:40px;border: 1px solid #e9e9e9; cursor: pointer;}
#two_tabs div a{display:block;}
#two_tabs div.active{background-color: #208ac8 !important;border: 1px solid #208ac8;}
#two_tabs div.active a{color: #fff;}
#two_tabs div:hover {background-color: #e9e9e9;}

.mOpn {display: none;}

